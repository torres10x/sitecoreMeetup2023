@model  List<Meetup.Models.Navigation.NavigationItem>
<h4 class="text-uppercase">Other events you will like</h4>
<div class="sidebar">
    <ul class="nav nav-pills nav-stacked">
        @foreach (var item in Model)
        {
            <li>
                <a href="@item.Url">@item.Title</a>
            </li>
        }

    </ul>
</div>
