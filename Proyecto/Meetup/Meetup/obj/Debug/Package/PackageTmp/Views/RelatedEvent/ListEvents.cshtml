@model List<Meetup.Models.EventIntro.EventDetails>


    <article>
        <!-- [Event List] -->
        <div class="content-list newslist">
            <ol class="media-list">
                @foreach (var item in Model)
                {
    <li class="media media-align-middle">
        <div class="media-left">
            <a href="@item.ContentIntro">
                <img src="@item.EventImage"/>
            </a>

        </div>
        <div class="media-body">
            <h3 class="media-heading">
                <a href="@item.ContentIntro">@item.ContentHeading</a>
            </h3>
            <div class="description">
                <p>@item.ContentBody</p>
            </div>
        </div>
        <div class="metadata well">@item.StartDate</div>
    </li>}

            </ol>
        </div>
        <!-- [Event List] -->
        <!-- [Pagination Navigation] -->
        <nav class="text-center">
            <ul class="pagination">
                <li class=disabled>
                    <a href="#" aria-label="Previous">
                        <span aria-hidden="true">&laquo;</span>
                    </a>
                </li>
                <li class="active"><a href="#">1</a></li>
                <li><a href="#">2</a></li>
                <li><a href="#">3</a></li>
                <li><a href="#">4</a></li>
                <li><a href="#">5</a></li>
                <li>
                    <a href="#" aria-label="Next">
                        <span aria-hidden="true">&raquo;</span>
                    </a>
                </li>
            </ul>
        </nav>
        <!-- [/Pagination Navigation] -->
    </article>


