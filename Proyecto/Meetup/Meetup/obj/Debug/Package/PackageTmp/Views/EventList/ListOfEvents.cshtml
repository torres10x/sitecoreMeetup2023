@model List<Meetup.Models.EventIntro.EventDetails>

<section class="section section-full">
    <!-- [Category List] -->

    <ul class="block-grid-md-4 block-grid-sm-3 block-grid-xs-1">
        @foreach (var item in Model)
        {
            <li>
                <div class="thumbnail">
                    <a href="@item.ContentBody">

                        @item.EventImage

                    </a>

                    <div class="caption">
                        <h3 class="teaser-heading">@item.ContentHeading</h3>
                        <a href="Event-List.html" class="btn btn-default">Read more</a>
                    </div>
                </div>
            </li>

        }
    </ul>
    <!-- [Category List] -->
</section>

